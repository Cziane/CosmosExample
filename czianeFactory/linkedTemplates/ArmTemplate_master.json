{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "czianeFactory"
		},
		"SQLCosmoDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SQLCosmoDB'"
		},
		"MongoCosmoDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MongoCosmoDB'"
		},
		"MongoCosmoDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Family"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "czianeFactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SQLCosmoDB_connectionString": {
						"value": "[parameters('SQLCosmoDB_connectionString')]"
					},
					"MongoCosmoDB_connectionString": {
						"value": "[parameters('MongoCosmoDB_connectionString')]"
					},
					"MongoCosmoDB_properties_typeProperties_database": {
						"value": "[parameters('MongoCosmoDB_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}